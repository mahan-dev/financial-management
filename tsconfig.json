{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/public/*": ["public/*"],
      "@/app/*": ["app/*"],
      "@/elements/*": ["app/components/elements/*"],
      "@/modules/*": ["app/components/modules/*"],
      "@/templates/*": ["app/components/templates/*"],
      "@/layout/*": ["app/components/layout/*"],
      "@/utils/*":["app/utils/*"],
      
      "@/models/*": ["app/models/*"],


    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "postcss.config.js", "tailwind.config.ts"],
  "exclude": ["node_modules"]
}
